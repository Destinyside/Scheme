	 (define (exec eta iter)
	   (let* ([eta eta]
		  [iter iter]
		  [attribute-count 4]
		  [hidden-count (+ attribute-count 8)]
		  [type-count 3]
		  [counts (make-a-counts attribute-count hidden-count type-count)]
		  [bpnn (make-a-neuron-network counts)]
		  [train-nodes (init-train-nodes)])
	     (train-nodes-set! bpnn train-nodes)
	     (train bpnn counts eta iter)
	     (display bpnn)
	     (newline)
	     (let* ([test-nodes (init-test-nodes)]
		    [len (length test-nodes)]
		    [right 0])
	       (map (lambda (x) 
		      (let ([type (test bpnn x)])
			(if (= type (data-node-type x))
			  (set! right (+ right 1)))
			(display type)
			(newline)))
		    test-nodes)
	       (newline)
	       (display "per : ")
	       (display (/ right len 1.0))
	       (newline)))
	   )


